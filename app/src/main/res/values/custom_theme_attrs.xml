<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- ===============自定义主题换肤的自定义属性文件====================  -->
    <!--通常格式是：-->
    <!--name="自定义属性名称" format="资源引用格式(color,dimen,reference)"-->


    <!-- 控制app背景色 format:颜色值、资源引用 -->
    <attr name="custom_attr_app_bg" format="color|reference" />
    <!-- 控制app标题栏背景色  -->
    <attr name="custom_attr_app_toolbar_bg" format="color|reference" />
    <!-- 控制apph状态栏栏背景色  -->
    <attr name="custom_attr_app_statusbar_bg" format="color|reference" />
    <!-- 控制app按钮的背景色 -->
    <attr name="custom_attr_app_btn_bg" format="color|reference" />
    <!-- 控制app字体颜色 -->
    <attr name="custom_attr_app_textcolor" format="color|reference" />

    <!-- toolbar 图标自定义属性 -->
    <attr name="custom_attr_toolbar_notify" format="reference"/>
    <attr name="custom_attr_toolbar_search" format="reference"/>
    <attr name="custom_attr_toolbar_qrcode" format="reference"/>




</resources>