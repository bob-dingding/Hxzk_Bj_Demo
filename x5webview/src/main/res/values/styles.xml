<resources>

    <!-- Base application theme. -->
    <style name="X5AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- API Level 22 编译要去点android -->
        <item name="windowNoTitle">true</item>
        <!--  <item name="android:windowNoTitle">true</item>-->
        <item name="windowActionBar">false</item>
    </style>



    <style name="AppTheme.Day" parent="X5AppTheme">
        <item name="custom_attr_app_statusbar_bg">@color/custom_color_app_status_bg_day</item>

    </style>



    <style name="AppTheme.Night" parent="X5AppTheme">
        <item name="custom_attr_app_statusbar_bg">@color/custom_color_app_status_bg_night</item>
    </style>

</resources>
